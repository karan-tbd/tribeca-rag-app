# Vite client-side env vars (safe to expose in browser)
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# Supabase Edge Functions (server-side)
# Used only when running/deploying edge functions; DO NOT commit real values
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=

# OpenAI API (for embeddings and chat completion)
OPENAI_API_KEY=

# Linear: token is stored per-user in DB (users.linear_api_token)
# Do not put a global Linear API key here unless you know what you're doing.

# PDF Processing Configuration
MAX_CHUNK_SIZE=1000
CHUNK_OVERLAP=200
EMBEDDING_MODEL_FALLBACK=text-embedding-3-small  # Fallback if agent has no embedding model configured
MAX_CONCURRENT_CHUNKS=5

